{
  "name": "AdBlock-Detector",
  "version": "1.0",
  "description": "Simple detector",
  "permissions": ["activeTab","<all_urls>"],  
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content-script.js"],
      "css": ["styles.css"],
      "run_at": "document_end"

    }
  ],
  "manifest_version": 2
}